// Leetcode 198
class Solution {
       public int rob(int[] nums) {
//      int [] memo = new int[nums.length];
//        Arrays.fill(memo , -1);
        int previousByTwo = 0 , previous = nums[0] , current = 0;
        for(int i = 1; i < nums.length; i++) {
           if(i>1) {
               current = Math.max(nums[i] + previousByTwo, previous);
               previousByTwo = previous;
               previous = current;
           }
        else {
            previousByTwo = previous;
            previous = Math.max(nums[i] , nums[i-1]);
           }
        }
        return previous;
    }
}
